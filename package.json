{
  "name": "@jordilo/daily-standup-timer",
  "version": "0.1.2",
  "main": "dist/main.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build-dev": "ng build",
    "build-electron": "tsc -p electron && electron ./dist/main.js",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "only-electon": "electron dist/main.js",
    "build": "ng build --prod && tsc -p electron && electron-builder --dir -w",
    "electron": "ng build && npm run build-electron",
    "postinstall": "electron-builder install-app-deps",
    "update-version": "npm version patch && now -e ACCOUNT=\"jordilo\" -e REPOSITORY=\"daily-standup-timer\""
  },
  "build": {
    "asar": false,
    "productName": "Daily Standup timer",
    "appId": "com.electron.jordilo-daily-standup-timer",
    "files": [
      "!node_modules/",
      "!src/",
      "dist/**/*"
    ],
    "directories": {
      "output": "release",
      "buildResources": "dist/daily-standup-timer"
    },
    "mac": {
      "category": "com.electron.jordilo-daily-standup-timer.type"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.1.0",
    "@angular/common": "~9.1.0",
    "@angular/compiler": "~9.1.0",
    "@angular/core": "^9.1.6",
    "@angular/forms": "~9.1.0",
    "@angular/platform-browser": "~9.1.0",
    "@angular/platform-browser-dynamic": "~9.1.0",
    "@angular/router": "~9.1.0",
    "lodash": "^4.17.15",
    "moment": "^2.25.3",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^9.1.0",
    "@angular-devkit/build-angular": "~0.901.0",
    "@angular/cli": "^9.1.5",
    "@angular/compiler-cli": "~9.1.0",
    "@angular/language-service": "~9.1.0",
    "@types/electron": "^1.6.10",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "electron": "^8.2.5",
    "electron-builder": "^22.6.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~3.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "protractor": "~5.4.3",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~3.8.3"
  }
}
